<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <gradient
            android:angle="270"
            android:centerColor="#FFB0E2FF"
            android:centerY="0.75"
            android:endColor="#FFB2DFEE"
            android:startColor="#FFB0E0E6"
                />
        </shape>
    </item>


    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient
                    android:angle="270"
                    android:centerColor="#2F85C2"
                    android:centerY="0.75"
                    android:endColor="#2F85C2"
                    android:startColor="#2F85C2"
                    />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient
                    android:angle="270"
                    android:centerColor="#00E6E6"
                    android:centerY="0.75"
                    android:endColor="#00E6E6"
                    android:startColor="#00E6E6"
                    />
            </shape>
        </clip>
    </item>

</layer-list>


